            <Image url="@assets/setting.png" />
         </image>
      </ImageView>
      <Button layoutX="143.0" layoutY="57.0" mnemonicParsing="false" onAction="#onActionManageUser" prefHeight="45.0" prefWidth="187.0" style="-fx-background-color: #012f74; -fx-background-radius: 40;" text="Change User Password" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button layoutX="397.0" layoutY="57.0" mnemonicParsing="false" onAction="#onActionAddUser" prefHeight="45.0" prefWidth="187.0" style="-fx-background-color: #012f74; -fx-background-radius: 40;" text="Create User Account" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
